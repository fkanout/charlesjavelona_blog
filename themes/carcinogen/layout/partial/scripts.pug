    function toggleOpen () {
        var element = $("#nav-list")
        
        console.log("Element data-open is: " + element.attr('data-open'))
        

        if (element.attr('data-open') == "true") {
            console.log("Drawer open, closing it")
             element.addClass("closed")
             element.attr('data-open', false);
        }
        else {
            console.log("Drawer closed, opening it")
            element.removeClass("closed")
            element.attr('data-open', true);
        }
    }
